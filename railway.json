{
  "services": {
    "frontend": {
      "build": {
        "builder": "dockerfile",
        "dockerfilePath": "Dockerfile",
        "context": "front-diet/frontend"
      },
      "deploy": {
        "restartPolicyType": "on_failure",
        "restartPolicyMaxRetries": 3
      }
    },
    "backend": {
      "build": {
        "builder": "dockerfile",
        "dockerfilePath": "back-diet/backend/demo/Dockerfile"
      }
    }
  }
}
